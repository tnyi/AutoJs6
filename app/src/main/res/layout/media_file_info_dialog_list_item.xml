<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:focusable="true"
    android:clickable="true"
    android:gravity="center_vertical|start"
    android:minHeight="@dimen/ref_md_listitem_height"
    android:paddingBottom="@dimen/ref_md_listitem_vertical_margin"
    android:paddingTop="@dimen/ref_md_listitem_vertical_margin">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/container_format_parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/ref_md_listitem_margin_left"
        android:layout_marginEnd="@dimen/ref_md_dialog_frame_margin"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@id/video_format_parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/container_format_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="100dp" />

        <TextView
            android:id="@+id/container_format_label"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|start"
            android:lineSpacingExtra="2dp"
            android:text="@string/ellipsis_six"
            android:textAlignment="viewStart"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/container_format_guideline"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="@string/media_info_container_format_label" />

        <TextView
            android:id="@+id/container_format_colon"
            android:visibility="gone"
            tools:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="4sp"
            android:gravity="center_vertical|start"
            android:text="@string/symbol_colon"
            android:textAlignment="viewStart"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/container_format_value"
            app:layout_constraintStart_toEndOf="@id/container_format_label"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/container_format_value"
            android:visibility="gone"
            tools:visibility="visible"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:gravity="center_vertical|start"
            android:lineSpacingExtra="2dp"
            android:maxLines="2"
            android:paddingTop="0dp"
            android:paddingBottom="0dp"
            android:text="@string/ellipsis_six"
            tools:text="@string/text_unknown"
            android:textAlignment="viewStart"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/container_format_colon"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/video_format_parent"
        android:visibility="gone"
        tools:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/ref_md_dialog_frame_margin"
        android:layout_marginStart="@dimen/ref_md_listitem_margin_left"
        android:layout_marginTop="4sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/container_format_parent"
        app:layout_constraintBottom_toTopOf="@id/audio_format_parent"
        app:layout_constraintEnd_toEndOf="parent">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/video_format_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="100dp" />

        <TextView
            android:id="@+id/video_format_label"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:text="@string/media_info_video_format_label"
            android:lineSpacingExtra="2dp"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/video_format_guideline" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/video_format_colon"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:layout_marginHorizontal="4sp"
            android:text="@string/symbol_colon"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintStart_toEndOf="@id/video_format_label"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/video_format_value" />

        <TextView
            android:id="@+id/video_format_value"
            android:text="@string/ellipsis_six"
            tools:text="@string/text_unknown"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:maxLines="2"
            android:ellipsize="end"
            android:paddingBottom="0dp"
            android:paddingTop="0dp"
            android:lineSpacingExtra="2dp"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintStart_toEndOf="@id/video_format_colon"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/audio_format_parent"
        android:visibility="gone"
        tools:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/ref_md_dialog_frame_margin"
        android:layout_marginStart="@dimen/ref_md_listitem_margin_left"
        android:layout_marginTop="4sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/video_format_parent"
        app:layout_constraintBottom_toTopOf="@id/file_size_parent"
        app:layout_constraintEnd_toEndOf="parent">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/audio_format_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="100dp" />

        <TextView
            android:id="@+id/audio_format_label"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:text="@string/media_info_audio_format_label"
            android:lineSpacingExtra="2dp"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/audio_format_guideline" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/audio_format_colon"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:layout_marginHorizontal="4sp"
            android:text="@string/symbol_colon"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintStart_toEndOf="@id/audio_format_label"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/audio_format_value" />

        <TextView
            android:id="@+id/audio_format_value"
            android:text="@string/ellipsis_six"
            tools:text="@string/text_unknown"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:maxLines="2"
            android:ellipsize="end"
            android:paddingBottom="0dp"
            android:paddingTop="0dp"
            android:lineSpacingExtra="2dp"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintStart_toEndOf="@id/audio_format_colon"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/file_size_parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/ref_md_listitem_margin_left"
        android:layout_marginTop="4sp"
        android:layout_marginEnd="@dimen/ref_md_dialog_frame_margin"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@id/duration_parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/audio_format_parent">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/file_size_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="100dp" />

        <TextView
            android:id="@+id/file_size_label"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|start"
            android:lineSpacingExtra="2dp"
            android:text="@string/ellipsis_six"
            android:textAlignment="viewStart"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/file_size_guideline"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="@string/media_info_file_size_label" />

        <TextView
            android:id="@+id/file_size_colon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="4sp"
            android:gravity="center_vertical|start"
            android:text="@string/symbol_colon"
            android:textAlignment="viewStart"
            android:textSize="@dimen/ref_md_listitem_textsize"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/file_size_value"
            app:layout_constraintStart_toEndOf="@id/file_size_label"
            app:layout_constraintTop_toTopOf="parent"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/file_size_value"
            android:visibility="gone"
            tools:visibility="visible"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:gravity="center_vertical|start"
            android:lineSpacingExtra="2dp"
            android:maxLines="2"
            android:paddingTop="0dp"
            android:paddingBottom="0dp"
            android:text="@string/ellipsis_six"
            tools:text="@string/text_unknown"
            android:textAlignment="viewStart"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/file_size_colon"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/duration_parent"
        android:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/ref_md_dialog_frame_margin"
        android:layout_marginStart="@dimen/ref_md_listitem_margin_left"
        android:layout_marginTop="4sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/file_size_parent"
        app:layout_constraintBottom_toTopOf="@id/bit_rate_for_audio_parent"
        app:layout_constraintEnd_toEndOf="parent">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/duration_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="100dp" />

        <TextView
            android:id="@+id/duration_label"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:text="@string/ellipsis_six"
            tools:text="@string/media_info_duration_label"
            android:lineSpacingExtra="2dp"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/duration_guideline" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/duration_colon"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:layout_marginHorizontal="4sp"
            android:text="@string/symbol_colon"
            android:textSize="@dimen/ref_md_listitem_textsize"
            android:visibility="gone"
            tools:visibility="visible"
            app:layout_constraintStart_toEndOf="@id/duration_label"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/duration_value" />

        <TextView
            android:id="@+id/duration_value"
            android:visibility="gone"
            tools:visibility="visible"
            android:text="@string/ellipsis_six"
            tools:text="@string/text_unknown"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:maxLines="2"
            android:ellipsize="end"
            android:paddingBottom="0dp"
            android:paddingTop="0dp"
            android:lineSpacingExtra="2dp"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintStart_toEndOf="@id/duration_colon"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bit_rate_for_audio_parent"
        android:visibility="gone"
        tools:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/ref_md_dialog_frame_margin"
        android:layout_marginStart="@dimen/ref_md_listitem_margin_left"
        android:layout_marginTop="4sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/duration_parent"
        app:layout_constraintBottom_toTopOf="@id/split_line"
        app:layout_constraintEnd_toEndOf="parent">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/bit_rate_for_audio_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="100dp" />

        <TextView
            android:id="@+id/bit_rate_for_audio_label"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:text="@string/media_info_bit_rate_label"
            android:lineSpacingExtra="2dp"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/bit_rate_for_audio_guideline" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/bit_rate_for_audio_colon"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:layout_marginHorizontal="4sp"
            android:text="@string/symbol_colon"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintStart_toEndOf="@id/bit_rate_for_audio_label"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/bit_rate_for_audio_value" />

        <TextView
            android:id="@+id/bit_rate_for_audio_value"
            android:text="@string/ellipsis_six"
            tools:text="@string/text_unknown"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:maxLines="2"
            android:ellipsize="end"
            android:paddingBottom="0dp"
            android:paddingTop="0dp"
            android:lineSpacingExtra="2dp"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintStart_toEndOf="@id/bit_rate_for_audio_colon"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <View
        android:id="@+id/split_line"
        android:visibility="gone"
        tools:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/day_night_alpha_20"
        android:layout_marginEnd="@dimen/ref_md_dialog_frame_margin"
        android:layout_marginStart="@dimen/ref_md_listitem_margin_left"
        android:layout_marginTop="8sp"
        android:layout_marginBottom="3sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/bit_rate_for_audio_parent"
        app:layout_constraintBottom_toTopOf="@id/bit_rate_for_video_parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bit_rate_for_video_parent"
        android:visibility="gone"
        tools:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/ref_md_dialog_frame_margin"
        android:layout_marginStart="@dimen/ref_md_listitem_margin_left"
        android:layout_marginTop="4sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/split_line"
        app:layout_constraintBottom_toTopOf="@id/resolution_parent"
        app:layout_constraintEnd_toEndOf="parent">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/bit_rate_for_video_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="100dp" />

        <TextView
            android:id="@+id/bit_rate_for_video_label"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:text="@string/media_info_bit_rate_label"
            android:lineSpacingExtra="2dp"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/bit_rate_for_video_guideline" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/bit_rate_for_video_colon"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:layout_marginHorizontal="4sp"
            android:text="@string/symbol_colon"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintStart_toEndOf="@id/bit_rate_for_video_label"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/bit_rate_for_video_value" />

        <TextView
            android:id="@+id/bit_rate_for_video_value"
            android:text="@string/ellipsis_six"
            tools:text="@string/text_unknown"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:maxLines="2"
            android:ellipsize="end"
            android:paddingBottom="0dp"
            android:paddingTop="0dp"
            android:lineSpacingExtra="2dp"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintStart_toEndOf="@id/bit_rate_for_video_colon"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/resolution_parent"
        android:visibility="gone"
        tools:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/ref_md_dialog_frame_margin"
        android:layout_marginStart="@dimen/ref_md_listitem_margin_left"
        android:layout_marginTop="4sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/bit_rate_for_video_parent"
        app:layout_constraintBottom_toTopOf="@id/aspect_ratio_parent"
        app:layout_constraintEnd_toEndOf="parent">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/resolution_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="100dp" />

        <TextView
            android:id="@+id/resolution_label"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:text="@string/media_info_resolution_label"
            android:lineSpacingExtra="2dp"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/resolution_guideline" />

        <TextView
            android:id="@+id/resolution_colon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:layout_marginHorizontal="4sp"
            android:text="@string/symbol_colon"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintStart_toEndOf="@id/resolution_label"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/resolution_value" />

        <TextView
            android:id="@+id/resolution_value"
            android:text="@string/ellipsis_six"
            tools:text="@string/text_unknown"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|start"
            android:textAlignment="viewStart"
            android:maxLines="2"
            android:ellipsize="end"
            android:paddingBottom="0dp"
            android:paddingTop="0dp"
            android:lineSpacingExtra="2dp"
            android:textSize="@dimen/ref_md_listitem_textsize"
            app:layout_constraintStart_toEndOf="@id/resolution_colon"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/aspect_ratio_parent"
        android:visibility="gone"
        tools:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/ref_md_dialog_frame_margin"
        android:layout_marginStart="@dimen/ref_md_listitem_margin_left"
        android:layout_marginTop="4sp"
        app:layout_constraintStart_toStartOf="parent"
